{
  "extends": "standard",
  "env": {
    "es6": true,
    "node": true
  },
  "globals": {
    "document": false,
    "window": false,
    "console": false,
    "FontFaceObserver": false,
    "APP_DATA": false,
    "State": false,
    "App": false,
    "Application": false,
    "Intro": false,
    "Game": false,
    "Level": false,
    "Player": false,
    "Enemy": false,
    "About": false,
    "Credits": false
  },
  "rules": {
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-unused-vars": [
      0
    ],
    "no-console": [
      0
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}
